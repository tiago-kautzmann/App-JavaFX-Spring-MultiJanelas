<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TextField?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.demo.controllers.contato.TelaAdicionarContatoController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
  </padding>

  <Label text="Adicionar Contato" style="-fx-font-size: 20px;"/>

  <HBox spacing="10.0">
    <Label text="Descrição"/>
    <TextField fx:id="textFieldDescricao"/>
  </HBox>

  <HBox spacing="10.0">
    <Label text="Tipo"/>
    <TextField fx:id="textFieldTipo"/>
  </HBox>

  <Button text="Adicionar" onAction="#onAdicionarContatoButtonClick"/>

</VBox>
